<div class="page-header">
  <h1>Currently Loggedin In User List</h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Picture</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td> <%= user.name %> </td>
        <td> <%= user.email %> </td>
        <td> 
          <div class="profile_picture">
            <%= image_tag user.profile_image, class: 'profile-img img-circle'  %>
          </div>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>